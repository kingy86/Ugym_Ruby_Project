<table class="membertable">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Age</th>
    <th>Fitness Level</th>
    <th>Session</th>
  </tr>
    <% @member.each do |member|%>
    <tr>
      <td><%= member.first_name %></td>
      <td><%= member.last_name %></td>
      <td><%= member.age %></td>
      <td><%= member.fitness_level %></td>
      <td><%= member.session_name %></td>
      <td><a href="members/<%=member.id%>">Edit<a></td>
    </tr>
    <%end%>
</table>
